    "pouchdb-plugin-error": "^1.0.0",
    "pouchdb-promise": "^5.4.0",
    "pouchdb-req-http-query": "^2.0.0",
    "pouchdb-system-db": "^1.0.0",
    "pouchdb-validation": "^1.1.0",
    "pouchdb-wrappers": "^1.0.0",
    "promise-nodify": "^1.0.0"
  },
  "devDependencies": {
    "extend": "^3.0.0",
